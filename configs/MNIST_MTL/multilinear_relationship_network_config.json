{
  "activation": "relu",
  "batch_size": 512,
  "bottom": {
    "activation": "relu",
    "dropout_feature": 0.1,
    "dropout_hidden": 0.2,
    "hidden_dimension": 64
  },
  "covariance_epsilon": 1e-05,
